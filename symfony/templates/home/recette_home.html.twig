    {% set size = create_recettes | length %}
    {% if size > 0 %}
    <table class="table">
        <thead>
            <tr>
                <th>Nom</th>
                <th>Présentation</th>
                <th>Note</th>
                <th>Voir</th>
            </tr>
        </thead>
        <tbody>

            <tr>
            {% for create_recette in create_recettes | slice (0, 3) %}
                <td>{{ create_recette.TitreRecette }}</td>
                <td>{{ create_recette.Presentation }}</td>
                <td>
                    {% set size2 = create_recette.ratings | length %}
                    {% if size2 > 0 %}
                        {% set somme = 0.0 %}
                        {% set compteur = 0 %}
                        {% for ratings in create_recette.ratings %}
                            {% set somme = somme + ratings.note %}
                            {% set compteur = compteur + 1 %}
                        {% endfor %}
                        {{ somme / compteur }}
                    {% else %}
                        Il n'y a pas de notes.
                    {% endif %}
                </td>
                <td>
                    <a href="{{ path('create_recette_show', {'id': create_recette.id}) }}">show</a>
                </td>
            </tr>
                    {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}

        </tbody>
    </table>
    {% else %}
        Il n'y a pas encore de recettes créer sur notre site.
    {% endif %}



